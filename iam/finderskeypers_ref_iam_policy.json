{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "FindersKeypersRead",
            "Effect": "Allow",
            "Action": [
                "kms:ListKeys",
                "kms:ListAliases",
                "kms:DescribeKey",
                "kms:GetKeyPolicy",
                "glue:GetDataCatalogEncryptionSettings",
