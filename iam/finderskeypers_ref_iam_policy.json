                "s3tables:GetTableEncryption",
                "ec2:DescribeVolumes",
                "qldb:ListLedgers",
                "qldb:DescribeLedger",
                "redshift:DescribeClusters",
                "redshift-serverless:ListNamespaces",
                "cassandra:Select",
                "timestream:ListDatabases",
                "timestream-influxdb:ListDbInstances",
